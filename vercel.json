{
  "version": 2,
  "builds": [
    {
      "src": "next.config.js",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1",
      "methods": ["GET", "POST", "PUT", "PATCH", "DELETE"],
      "headers": {
        "Cache-Control": "no-store",
        "Access-Control-Allow-Origin": "*"
      },
      "continue": true
    }
  ],
  "functions": {
    "api/**/*.js": {
      "memory": 512,
      "maxDuration": 60
    }
  }
}
